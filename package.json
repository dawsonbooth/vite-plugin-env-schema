{
  "name": "vite-plugin-env-schema",
  "version": "0.1.0",
  "description": "Build-time environment variable validation and injection for Vite",
  "repository": "https://github.com/dawsonbooth/vite-plugin-env-schema",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "lint:format": "prettier --check .",
    "lint:code": "eslint . --max-warnings=0",
    "lint:types": "tsc --noEmit",
    "lint": "concurrently -g -c auto -n \"format,code,types\" \"npm run lint:format\" \"npm run lint:code\" \"npm run lint:types\"",
    "test": "bun test"
  },
  "devDependencies": {
    "@standard-schema/spec": "^1.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-oxlint": "^1.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.3",
    "zod": "^3.25.67"
  },
  "peerDependencies": {
    "vite": ">= 5.0"
  }
}
